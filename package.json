{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"npm run remix:dev\"",
    "remix:dev": "node -r dotenv/config ./node_modules/@remix-run/dev/cli.js",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@remix-run/react": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.7",
    "firebase-admin": "^10.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
