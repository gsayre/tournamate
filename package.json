{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "remix:dev": "remix dev",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.4",
    "@remix-run/react": "^1.6.4",
    "@remix-run/serve": "^1.6.4",
    "firebase": "^9.9.0",
    "firebase-admin": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.6.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.4",
    "@remix-run/eslint-config": "1.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "concurrently": "^7.2.2",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "firebase-tools": "^11.2.1",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.5",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
